Name: <%= @menu.name %><br>
Price: <%= @menu.price %><br>
Description: <%= @menu.description %><br>

<%= form_tag('/carts/add', method: :post) do %>
 <%= label_tag :数量 %>
 <%= select :cart, :quantity, [1,2,3] %><br>
 <%= hidden_field_tag :menuId, @menuId %><br>
 <%= submit_tag "追加する" %><br>
<% end %>
